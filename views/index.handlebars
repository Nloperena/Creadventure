                        var newUser = {
                            username: username,
                            password: password
                        }

                        $.ajax({
                            url:"/login/"+username, 
                            type: "GET",
                        }).then(function(data){
                          console.log(data)
                           $("html").html(data)
                        })
                    })

                    $("#submit-signup").on("click", function(){
